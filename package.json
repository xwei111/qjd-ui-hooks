{
  "name": "qjd-ui-hooks",
  "version": "1.0.2",
  "description": "UI for Vue 2.x, Hooks for composition-api",
  "author": "xw",
  "main": "lib/qjdui.js",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "clean": "rimraf lib",
    "clean:theme": "rimraf packages/theme-default/lib",
    "build": "node build/build.js",
    "build:theme": "npm run clean:theme && gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "build:packages": "webpack --config build/lib.config.js",
    "build:ui": "npm run clean && npm run build:theme && npm run build:packages",
    "release": "standard-version --release-as",
    "release:100": "npm run release -- major",
    "release:010": "npm run release -- minor",
    "release:001": "npm run release -- patch"
  },
  "style": "lib/theme-default/index.css",
  "files": [
    "lib"
  ],
  "keywords": [
    "qjd-ui",
    "vue2.x"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.4",
    "@vueuse/core": "^7.4.0",
    "clipboard": "^2.0.8",
    "vue": "^2.5.2",
    "vue-fragment": "^1.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@vue/test-utils": "^1.1.3",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
